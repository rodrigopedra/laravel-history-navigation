{
  "name": "rodrigopedra/laravel-history-navigation",
  "description": "History navigation inspired by JavaScript's History API",
  "keywords": [
    "Laravel",
    "History",
    "History navigation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Rodrigo Pedra Brum",
      "email": "rodrigo.pedra@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0",
    "illuminate/contracts": "^10.0",
    "illuminate/http": "^10.0",
    "illuminate/routing": "^10.0",
    "illuminate/auth": "^10.0",
    "jakeasmith/http_build_url": "^1.0.1"
  },
  "autoload": {
    "files": [
      "bootstrap/helpers.php"
    ],
    "psr-4": {
      "RodrigoPedra\\HistoryNavigation\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RodrigoPedra\\HistoryNavigation\\HistoryNavigationServiceProvider"
      ]
    }
  }
}
