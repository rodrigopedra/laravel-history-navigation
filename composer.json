{
  "name": "rodrigopedra/laravel-history-navigation",
  "description": "History navigation inspired by JavaScript's History API",
  "keywords": [
    "Laravel",
    "History",
    "History navigation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Rodrigo Pedra Brum",
      "email": "rodrigo.pedra@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "^5.5|^6.0",
    "illuminate/contracts": "^5.5|^6.0",
    "illuminate/http": "^5.5|^6.0",
    "illuminate/routing": "^5.5|^6.0",
    "illuminate/auth": "^5.5|^6.0",
    "jakeasmith/http_build_url": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable"
  },
  "autoload": {
    "files": [
      "src/helpers.php",
      "src/HistoryNavigation/helpers.php"
    ],
    "psr-4": {
      "RodrigoPedra\\HistoryNavigation\\": "src/HistoryNavigation/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RodrigoPedra\\HistoryNavigation\\Providers\\HistoryNavigationServiceProvider"
      ]
    }
  }
}
